[checkout]
	defaultRemote = origin

[user]
	email = jrapodaca@gmail.com
	name = Jonathan Apodaca

[alias]
	conflicts = diff --name-only --diff-filter=U
	check-conflicts = diff --check
	hist = show --stat
	l = log --graph --oneline --branches --decorate --all
	s = status -s
	root = rev-parse --show-toplevel
